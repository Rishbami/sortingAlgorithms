def insertion_sort_oop(list_to_sort):
    new_list = []
    new_list.append(list_to_sort[0])
    for i in range(1, len(list_to_sort)):
        for j in range(len(new_list)):
            if list_to_sort[i] < new_list[j]:
                new_list.insert(j, list_to_sort[i])
                break
            else:
                pass
    return new_list
